<?xml version="1.0" encoding="utf-8"?>
<github.com.st235.swipetoactionlayout.swipe.SwipeToActionLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipeToAction"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:sal_gravity="right"
    app:sal_isFullActionSupported="false"
    app:sal_items="@menu/swipe_menu"
    app:sal_shouldVibrateOnQuickAction="false">

    <include
        android:id="@+id/data"
        layout="@layout/wallet_operation_data_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</github.com.st235.swipetoactionlayout.swipe.SwipeToActionLayout>